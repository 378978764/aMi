{
	"name": "ami",
	"publisher": "aLi",
	"displayName": "aMi",
	"description": "alternate Matlab ide",
	"icon": "images/aMi.png",
	"version": "0.0.0",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.startMatlab"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.startMatlab",
			"title": "Start Matlab",
			"category": "aMi"
		},
		{
			"command": "extension.stopMatlab",
			"title": "Stop Matlab",
			"category": "aMi"
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
